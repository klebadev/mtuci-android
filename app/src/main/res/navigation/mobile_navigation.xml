<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_number">

    <fragment
        android:id="@+id/navigation_number"
        android:name="ru.vvkleba.myapplication.ui.number.NumberFragment"
        android:label="@string/title_number"
        tools:layout="@layout/fragment_number" />


    <fragment
        android:id="@+id/navigation_list"
        android:name="ru.vvkleba.myapplication.ui.list.ListFragment"
        android:label="@string/title_list"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_dice"
        android:name="ru.vvkleba.myapplication.ui.dice.DiceFragment"
        android:label="@string/title_dice"
        tools:layout="@layout/fragment_dice" />


    <fragment
        android:id="@+id/navigation_coin"
        android:name="ru.vvkleba.myapplication.ui.coin.CoinFragment"
        android:label="@string/title_coin"
        tools:layout="@layout/fragment_coin" />

    <fragment
        android:id="@+id/navigation_ball"
        android:name="ru.vvkleba.myapplication.ui.ball.BallFragment"
        android:label="@string/title_ball"
        tools:layout="@layout/fragment_ball" />
</navigation>